{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "warn",
    "react/no-unescaped-entities": "off"
    // "react/react-in-jsx-scope": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": { "jsx": true },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}
